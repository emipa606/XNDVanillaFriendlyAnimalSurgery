  <RecipeDef ParentName="SurgeryAnimalFlesh" Name="InstallSimpleProstheticAnimalLegBase" Abstract="True">
    <label>install simple prosthetic leg</label>
    <description>Installs simple prosthetic leg.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Installing simple prosthetic leg.</jobString>
    <workAmount>2000</workAmount>
    <surgerySuccessChanceFactor>1.1</surgerySuccessChanceFactor>
    <skillRequirements>
      <Medicine>3</Medicine>
    </skillRequirements>
    <appliedOnFixedBodyParts>
      <li>Leg</li>
      <li>InsectLeg</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>
  <RecipeDef ParentName="InstallSimpleProstheticAnimalLegBase">
    <defName>InstallSimpleProstheticAnimalLegTiny</defName>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SimpleProstheticAnimalLegTiny</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>SimpleProstheticAnimalLegTiny</li>
      </thingDefs>
    </fixedIngredientFilter>
    <addsHediff>SimpleProstheticAnimalLegTiny</addsHediff>
  </RecipeDef>
